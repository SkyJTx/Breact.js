{
  "name": "@skyjt/breact",
  "version": "0.6.4",
  "description": "A full-stack framework using Bun, TypeScript, Elysia.js and a React-like frontend framework",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./shared": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "bin": {
    "breact": "dist/cli/index.js"
  },
  "scripts": {
    "build": "bun run build.ts",
    "build:lib": "bun build ./index.ts --outdir ./dist --format esm --sourcemap=inline",
    "build:cli": "bun build ./src/cli/index.ts --outdir ./dist/cli --format esm --sourcemap=inline",
    "check": "bun tsc --noEmit",
    "dev": "bun run check",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "framework",
    "fullstack",
    "bun",
    "typescript",
    "elysia",
    "react-like",
    "ssr",
    "csr"
  ],
  "author": "SkyJTx",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^27.0.0",
    "jsdom": "^27.2.0"
  },
  "peerDependencies": {
    "typescript": "^5",
    "elysia": "^1.4.0",
    "@elysiajs/swagger": "^1.3.0"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.5",
    "style-object-to-css-string": "^1.1.3"
  }
}
